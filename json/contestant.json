{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "range_contestant",
    "title": "Contestant",
    "description": "Validates that the contestant are returned in a consistent format",
    "type": "object",
    "properties": {
        "contestant_username": {
            "description": "The name of the contestant",
            "type": "string",
            "$comment": "primary key"
        },
        "password": {
            "description": "The contestant's password",
            "type": "string"
        },
        "contestant_first_name": {
            "description": "The contestant's first name",
            "type": "string"
        },
        "contestant_last_name": {
            "description": "The contestant's last name",
            "type": "string"
        }
    }, "required": ["contestant_username", "password", "contestant_first_name", "contestant_last_name"],
    "additionalProperties": false
}